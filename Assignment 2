#include <stdio.h>

int main() {

    int arr[]={10,20,10,20,30,20,40};

    int size=sizeof(arr)/sizeof(arr[0]);

    int ele,i;

    int freq=0;

    printf("Enter the element to find its frequency:10\t20\t30\n");

    scanf("%d",&ele);

    for(i=0;i<size;i++) {

        if(arr[i]==ele) {

            freq++;

        }

    }

    printf("\n%d\n",freq);

    return 0;

}
